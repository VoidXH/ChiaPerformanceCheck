<Window x:Class="ChiaPerformanceCheck.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChiaPerformanceCheck"
        mc:Ignorable="d"
        Title="Chia Performance Check" Height="450" Width="800">
    <Grid>
        <WpfPlot Name="histogram" Margin="0,40,0,0" />
        <Button Content="Load file" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="LoadFile"/>
        <TextBlock x:Name="results" HorizontalAlignment="Left" Margin="90,12,0,0" TextWrapping="Wrap" Text="Please load a file." VerticalAlignment="Top"/>
        <Button Content="by VoidX" Margin="0,10,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="Ad"/>
    </Grid>
</Window>
